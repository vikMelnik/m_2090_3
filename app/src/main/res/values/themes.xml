<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.M_2090_3" parent="Theme.Material3.DayNight.NoActionBar">

        <item name="chipStyle">@style/MyStyle</item>

        <!-- Primary brand color. -->
        <item name="colorPrimary">#763BFF</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="MyStyle" parent="Widget.Material3.Chip.Suggestion" >
        <item name="chipBackgroundColor">@drawable/my_selector_chipes</item>
        <item name="rippleColor">#FF5722</item>


    </style>

    <style name="Theme.M_2090_3.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.M_2090_3.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.M_2090_3.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>